<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.company.Car" table="carsDB">
        <id name="id">
            <generator class="assigned"></generator>
        </id>

        <property name="name"></property>
        <property name="cyl"></property>
        <property name="disp"></property>
        <property name="hp"></property>
        <property name="gear"></property>
        <property name="carb"></property>
        <property name="mpg"></property>
        <property name="drat"></property>
        <property name="wt"></property>
        <property name="qsec"></property>
        <property name="vs"></property>
        <property name="am"></property>
    </class>

</hibernate-mapping>